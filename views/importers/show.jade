extends ../layout

block content
    ol(class="breadcrumb")
        li
            a(href='/importers') Importers
        li(class="active")= importer.name
    h2=importer.name
    pre=JSON.stringify(importer.rules, null, 2)

    if(checkPermission('admin'))
        a(class="btn btn-default" href='/importers/#{importer.id}/edit') Edit
